{
    "plugins": [
        "prettier-plugin-tailwindcss",
        "prettier-plugin-organize-imports"
    ],
    "tailwindFunctions": [
        "classNames"
    ],
    "singleQuote": true,
    "eslintIntegration": true,
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "jsxBracketSameLine": false,
    "arrowParens": "avoid"
}